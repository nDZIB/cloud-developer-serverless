{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 3
    },
    "dueDate": {
      "type": "string",
      "minLength": 10,
      "pattern": "^\\d{4}(-\\d{2})(-\\d{2})$"
    }
  },
  "required": [
    "name",
    "dueDate"
  ],
  "additionalProperties": false
}
